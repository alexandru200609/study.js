2.
function sum(a, b) {
    return a + b;
}

// Test the function with different pairs of numbers
console.log(sum(5, 10)); // Expected output: 15
console.log(sum(-5, 5)); // Expected output: 0
console.log(sum(3.5, 2.5)); // Expected output: 6
console.log(sum(0, 0)); // Expected output: 0
console.log(sum(100, 200)); // Expected output: 300




